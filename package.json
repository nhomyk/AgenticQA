{
  "name": "agentic-qa-engineer",
  "version": "0.2.39",
  "description": "Production-ready Agentic QA Engineer: scans webpages for console errors, DOM issues, and security vulnerabilities",
  "main": "server.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhomyk/AgenticQA.git"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test:vitest": "vitest",
    "test:vitest:run": "vitest --run",
    "cypress": "cypress run",
    "test:cypress": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && start-server-and-test start 3000 cypress",
    "test:playwright": "playwright test",
    "test:jest": "jest --coverage",
    "test": "npm run test:jest && npm run test:vitest:run",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "coverage": "jest --coverage",
    "audit": "npm audit --audit-level=moderate || true",
    "audit:fix": "npm audit fix",
    "audit:report": "npm audit --json > audit-report.json",
    "test:pa11y": "pa11y-ci --config .pa11yci.json",
    "test:compliance": "npm run test:pa11y && npm run audit",
    "scan:compliance": "node run-compliance-scan.js",
    "agent": "node run-agent.js",
    "fullstack-agent": "node fullstack-agent.js",
    "qa:agent": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && npm run start > /tmp/server.log 2>&1 & sleep 3 && node qa-agent.js",
    "compliance-agent": "node compliance-agent.js",
    "docker:build": "docker build -t agentic-qa:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@langchain/core": "^1.1.12",
    "@langchain/langgraph": "^1.0.15",
    "@octokit/rest": "^22.0.1",
    "body-parser": "^1.20.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "langchain": "^1.2.7",
    "nodemailer": "^7.0.12",
    "puppeteer": "^24.35.0",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "cypress": "^15.8.2",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "pa11y": "^9.0.1",
    "pa11y-ci": "^4.0.1",
    "start-server-and-test": "^2.0.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}